int main() {
    double a, b, sonuc;
    printf("Sayı 1: ");
    scanf("%lf", &a);
    printf("Sayı 2: ");
    scanf("%lf", &b);  
 
    sonuc = a * b;
 
    printf("Sonuç = %.2lf", sonuc);
    
    return 0;
}